{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "target": "ES6",
    "lib": ["DOM", "ESNext"],
    "module": "ES2015",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "incremental": true,
    "declaration": false,
    "allowJs": false,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
